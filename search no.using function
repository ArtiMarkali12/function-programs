#include<stdio.h>
int main()
{
        int i,n,a[100],num;
        void search(int a[100],int num,int n);
        printf("enter limit :");
        scanf("%d",&n);
        printf("enter n numbers :");
        for(i=0;i<n;i++)
        {
        scanf("%d",&a[i]);
        }
        printf("enter number to search :");
        scanf("%d",&num);
        search(a,num,n);
        }
        void search(int a[100],int num,int n)
        {
        int i;
        for(i=0;i<n;i++)
        {
        if(a[i]==num)
        {
        printf("\n number is found:");
        break;
        }
        }
        if(i==n)
        printf("\n nuber is not found :");
        }
